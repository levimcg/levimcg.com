{
  "name": "levimcg.com",
  "version": "4.0.0",
  "description": "Levi McGranahan's personal website",
  "main": ".eleventy.js",
  "scripts": {
    "build": "ELEVENTY_ENV=prod npx eleventy && npm run sass:build",
    "clean": "rm -rf build",
    "serve": "ELEVENTY_ENV=dev npx eleventy --serve",
    "start": "run-p -c serve sass:dev",
    "sass:dev": "sass src/scss:build/css --watch",
    "sass:build": "sass --style=compressed src/scss:build/css",
    "fonts:copy": "node scripts/fonts.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@fontsource/public-sans": "^4.5.10",
    "@fontsource/source-code-pro": "^4.5.12",
    "@fontsource/source-sans-pro": "^4.5.11",
    "@fontsource/source-serif-pro": "^4.5.9",
    "fs-jetpack": "^4.3.1",
    "html-minifier": "^4.0.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.49.9"
  },
  "dependencies": {}
}
